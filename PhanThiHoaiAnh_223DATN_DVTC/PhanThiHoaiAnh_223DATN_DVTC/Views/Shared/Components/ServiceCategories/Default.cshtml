@model IEnumerable<ServiceCategoryModel>
@foreach(var item in Model)
{
	<div class="panel panel-default">
		<div class="panel-heading">
			<h4 class="panel-title">
				<a asp-action="Index" asp-controller="ServiceCategory" asp-route-Slug="@item.Slug">@item.CategoryName</a>
			</h4>
		</div>
	</div>
}